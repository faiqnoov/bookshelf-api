{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "no-trailing-spaces": ["error", { "ignoreComments": true, "skipBlankLines": true }],
        "no-console": "off"
    }
}
